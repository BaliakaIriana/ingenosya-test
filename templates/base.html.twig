<!DOCTYPE html>
<html>
<head>
    <meta charset="UTF-8">
    <title>{% block title %}Welcome!{% endblock %}</title>
    {# Run `composer require symfony/webpack-encore-bundle`
           and uncomment the following Encore helpers to start using Symfony UX #}
    {% block stylesheets %}
        {{ encore_entry_link_tags('app') }}
    {% endblock %}

    {% block javascripts %}
        {{ encore_entry_script_tags('app') }}
    {% endblock %}
</head>
<body>
<nav class="navbar navbar-dark bg-dark navbar-expand-lg ">
    <a class="navbar-brand p-2 m-1" href="{{ path('dirigeant_new') }}">Accueil</a>
    <div class="collapse navbar-collapse">
        <ul class="navbar-nav mr-auto">
            <li class="nav-item">
                <a class="nav-link" href="{{ path('dirigeant_index') }}">Liste dirigeants</a>
            </li>
            <li class="nav-item">
                <a class="nav-link" href="{{ path('societe_index') }}">Liste sociétés</a>
            </li>
        </ul>
    </div>
</nav>
{% block body %}{% endblock %}
</body>
</html>
